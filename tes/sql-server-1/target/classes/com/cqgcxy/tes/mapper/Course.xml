<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqgcxy.tes.mapper.CourseMapper">

    <insert id="insertCourse" parameterType="String">
        insert into course(name) values (#{name})
    </insert>

    <select id="selectCourseList" resultType="com.cqgcxy.tes.pojo.Course">
        select * from course
    </select>
</mapper>