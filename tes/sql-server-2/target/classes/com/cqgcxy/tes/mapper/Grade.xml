<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cqgcxy.tes.mapper.GradeMapper">

    <insert id="insertGrade" parameterType="String">
        insert into grade(name) values (#{name})
    </insert>

    <select id="selectGradeList" resultType="com.cqgcxy.tes.pojo.Grade">
        select * from grade
    </select>
</mapper>